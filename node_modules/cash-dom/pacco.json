{
  "browsers": ["ie >= 10"],
  "paths": {
    "tokens": {
      "bundle": "cash"
    },
    "output": {
      "javascript": {
        "unminified": "[dist]/[bundle].js",
        "minified": "[dist]/[bundle].min.js"
      }
    }
  },
  "plugins": {
    "wrapper": {
      "options": {
        "templates": {
          "default": "/* MIT https://github.com/kenwheeler/cash */\n(function(){\n[content]\n})();"
        }
      }
    }
  },
  "environments": {
    "esm": {
      "components": {
        "core/export_window": false
      },
      "paths": {
        "output": {
          "javascript": {
            "unminified": "[dist]/[bundle].esm.js",
            "minified": false
          }
        }
      },
      "plugins": {
        "wrapper": {
          "options": {
            "templates": {
              "default": "/* MIT https://github.com/kenwheeler/cash */\n[content]\nexport default cash;\n"
            }
          }
        }
      }
    },
    "production": {
      "plugins": {
        "uglify": {
          "enabled": false
        },
        "closure": {
          "enabled": true,
          "options": {
            "outputWrapper": "",
            "compilationLevel": "SIMPLE"
          }
        }
      }
    }
  }
}
